super + control + alt + Escape
	pkill -USR1 -x sxhkd ; pkill -USR1 -x polybar ; bspc wm -r 
	
super + q
	bspc node -c
super + alt + q
	bspc node -k

super + Escape
	bspc desktop -l next
super + control + Escape
	bspc node -t floating
super + alt + Escape
	bspc node -t tiled

super + Return
	tilix 
super + control + Return
	sudo tilix
super + alt + Return
	alacritty

F3
	xset dpms s off s noblank -dpms
F4
	xset dpms force off
super + space
	python ~/Documentos/Betas/python/keyboard.py

super + {1-3}
	bspc desktop -f '^{1-3}'
super + {8,9,0}
	bspc desktop -f '^{3-5}'

super + alt + {1-3}
	bspc desktop -f '^{6-8}'
super + alt + {8,9,0}
	bspc desktop -f '^{8,9,10}'

