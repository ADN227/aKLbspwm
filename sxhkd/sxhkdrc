super + shift + {1-9,0}
	bspc node -d '^{1-9,10}'

super + apostrophe
	bspc desktop -f last
super + period
	bspc desktop -f older

# focus
super + a 
	bspc node -f west
super + o
	bspc node -f south
super + comma
	bspc node -f north
super + e
	bspc node -f east

# move
super + alt + a
	bspc node -s west ; bspc node -f west
super + alt + o 
	bspc node -s south ; bspc node -f south
super + alt + comma
	bspc node -s north ; bspc node -f north
super + alt + e
	bspc node -s east ; bspc node -f east

# size 
super + control + a
	bspc node -z left -20 0; bspc node -z right -20 0
super + control + o
	bspc node -z bottom 0 20; bspc node -z top 0 20
super + control + comma
	bspc node -z bottom 0 -20; bspc node -z top 0 -20
super + control + e
	bspc node -z left 20 0; bspc node -z right 20 0


super + semicolon
	rofi -show drun -theme gnome
control + Escape
	rofi -show window -theme base

