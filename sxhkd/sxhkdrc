super + alt + w
	pactl set-sink-volume @DEFAULT_SINK@ -5%
super + alt + v
	pactl set-sink-mute @DEFAULT_SINK@ toggle
super + alt + z
	pactl set-sink-volume @DEFAULT_SINK@ +5%
super + alt + t
	playerctl previous
super + alt + n
	playerctl play-pause
super + alt + s
	playerctl next
super + alt + c
	scrot Capturas/K.jpg
super + alt + r
	scrot -d 10 Capturas/K.jpg
super + alt + l
	scrot -s Capturas/K.jpg

